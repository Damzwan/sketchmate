    "@capacitor/core": "^5.0.5",
    "@capacitor/filesystem": "^5.0.4",
    "@capacitor/haptics": "^5.0.4",
    "@capacitor/ios": "^5.0.5",
    "@capacitor/keyboard": "^5.0.5",
    "@capacitor/local-notifications": "^5.0.4",
    "@capacitor/network": "^5.0.6",
    "@capacitor/preferences": "^5.0.4",
    "@capacitor/push-notifications": "^5.0.4",
    "@capacitor/share": "^5.0.4",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.4",
    "@capacitor/toast": "^5.0.4",
    "@globalhive/vuejs-tour": "^1.3.6",
    "@hugotomazi/capacitor-navigation-bar": "^2.0.1",
    "@ionic/pwa-elements": "^3.1.1",
    "@ionic/vue": "^7.3.2",
    "@ionic/vue-router": "^7.0.12",
    "@vueuse/core": "^10.1.2",
    "autoprefixer": "^10.4.14",
    "browser-image-compression": "^2.0.2",
    "compressorjs": "^1.2.1",
    "cropperjs": "^1.5.13",
    "dayjs": "^1.11.8",
    "express": "^4.18.2",
    "fabric": "^5.3.0",
    "firebase": "^10.1.0",
    "fontfaceobserver": "^2.3.0",
    "lottie-web": "^5.12.2",
    "mitt": "^3.0.0",
