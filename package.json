{
  "name": "SketchMate",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "ionic serve",
    "start_ios": "ionic capacitor run ios -l --external",
    "start_android": "ionic capacitor run android -l --external",
    "build": "vite build && mkdir -p dist/.well-known && cp assetlinks.json dist/.well-known/assetlinks.json",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint",
    "heroku:start": "serve -s dist --single",
    "postinstall": "npm run build_fabric && npm run build",
    "build_fabric": "cd node_modules/fabric && npm run build_with_gestures"
  },
  "dependencies": {
    "@capacitor-mlkit/barcode-scanning": "^5.0.0",
    "@capacitor/android": "5.0.4",
    "@capacitor/app": "4.1.1",
    "@capacitor/camera": "^4.1.5",
    "@capacitor/clipboard": "^4.1.0",
    "@capacitor/core": "5.0.3",
    "@capacitor/filesystem": "^5.0.2",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.8.0",
    "@capacitor/local-notifications": "^4.1.5",
    "@capacitor/push-notifications": "^4.1.2",
    "@capacitor/share": "^4.1.1",
    "@capacitor/status-bar": "4.1.1",
    "@capacitor/toast": "^4.1.0",
    "@hugotomazi/capacitor-navigation-bar": "^2.0.1",
    "@ionic/pwa-elements": "^3.1.1",
    "@ionic/vue": "^7.0.11",
    "@ionic/vue-router": "^7.0.11",
    "@types/hammerjs": "^2.0.41",
    "@vueuse/core": "^10.1.2",
    "autoprefixer": "^10.4.14",
    "browser-image-compression": "^2.0.2",
    "compressorjs": "^1.2.1",
    "dayjs": "^1.11.8",
    "fabric": "^5.3.0",
    "fontfaceobserver": "^2.3.0",
    "hammerjs": "^2.0.8",
    "mitt": "^3.0.0",
    "pako": "^2.1.0",
    "pinia": "^2.1.3",
    "postcss": "^8.4.24",
    "q-floodfill": "^1.3.1",
    "qr-scanner": "^1.4.2",
    "qrcode.vue": "^3.4.0",
    "sass": "^1.63.2",
    "serve": "^14.2.0",
    "socket.io-client": "^4.6.2",
    "swiper": "^9.3.2",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@capacitor/cli": "4.8.0",
    "@mdi/js": "^7.2.96",
    "@types/fabric": "^5.3.3",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/pako": "^2.0.0",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "cypress": "^12.14.0",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "ionicons": "^7.1.2",
    "jsdom": "^21.1.2",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "uglify-js": "^3.17.4",
    "vite": "^4.3.9",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.6.5"
  },
  "description": "An Ionic project"
}
