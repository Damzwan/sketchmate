{
  "name": "SketchMate",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "ionic serve --external",
    "start_ios": "ionic capacitor run ios -l --external",
    "start_android": "ionic capacitor run android -l --external",
    "build": "vite build",
    "build_android": "vite build && npx cap sync android && npx cap copy android",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint",
    "lint_fix_and_add": "eslint --fix . && git add .",
    "heroku:start": "node server.js",
    "postinstall": "npm run build_fabric && npm run build",
    "build_fabric": "cd node_modules/fabric && npm run build_with_gestures",
    "prepare": "husky install",
    "heroku-prebuild": "echo 'force=true\nlegacy-peer-deps=true' > .npmrc"
  },
  "dependencies": {
    "@capacitor-mlkit/barcode-scanning": "^5.0.0",
