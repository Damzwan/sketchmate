    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.3",
    "ionicons": "^7.1.2",
    "jsdom": "^21.1.2",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "uglify-js": "^3.17.4",
    "vite": "^4.3.9",
